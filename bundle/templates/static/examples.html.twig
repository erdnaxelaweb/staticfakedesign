        <div class="row g-2 border-top m-2">
            {# @fake var161 block({'type': 'list'}) #}
            {# @fake var162 block('list') #}
            <div class="col-5 ">
                <div class="border bg-light g-2 p-2">
                <code class="chroma">
                    &#123;# @fake var block({'type': 'list'}) #&#125;<br/>
                    &#123;# @fake var block('list') #&#125;<br/>
                    &#123;&#123; var.name &#125;&#125;<br/>
                    &#123;&#123; var.fields }} &#125;&#125;
                </code>
                    </div>
            </div>
            <div class="col">
                {{ dump(var162.name) }}
                {{ dump(var162.fields) }}
            </div>
        </div>
        <div class="row g-2 border-top m-2">
            {# @fake var17 form #}
            <div class="col-5 ">
                <div class="border bg-light g-2 p-2">
                <code class="chroma">
                    &#123;# @fake var form #&#125;<br/>
                    &#123;&#123; form(var) &#125;&#125;<br/>
                </code>
                    </div>
            </div>
            <div class="col">
                {% form_theme var17 'bootstrap_5_layout.html.twig' %}
